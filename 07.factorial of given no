import java.io.*;
import java.util.Scanner;
class fact {
    public static void main(String[] args)
   {
        Scanner s = new Scanner(System.in);
        System.out.print("Enter the value of N: ");
        int n = s.nextInt();
        int res = 1;
        int i = n;
        while (i > 0) 
        {
            res *= i;
            i--;
        }
        System.out.println(n + " Factorial = " + res);
    }
}
