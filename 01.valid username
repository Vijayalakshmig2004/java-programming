import java.io.*;
import java.util.Scanner;
class bb
{
public static void main(String[] args)
{
      Scanner s=new Scanner(System.in);
      System.out.println(" enter username:");
      String s1=s.next();
      String s2=s.next();
      if  (s1==s2)
           System.out.println("username is valid");
      else
          System.out.println("username is invalid");
}
}

