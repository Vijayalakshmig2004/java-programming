import java.io.*;
import java.util.Scanner;
class sc
{
   public static void main(String[] args)
  {
      Scanner s=new Scanner(System.in);
      System.out.println("enter n:");
      float n;
      n=s.nextFloat();
      System.out.println("square:"+(n*n));
      System.out.println("cube:"+(n*n*n));
   }
}

