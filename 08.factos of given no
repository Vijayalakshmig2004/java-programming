import java.io.*;
import java.util.Scanner;
 class factor
 {
    public static void main(String[] args)
   {
        Scanner s = new Scanner(System.in);
        System.out.print("Enter the value of N: ");
        int n = s.nextInt();
        int fact=0,i=0;
        for(i=1;i<=n;i++)
        {
                if (n%i==0)
                 {
                        fact=fact+1;
                 }
         }
         System.out.println("factors:" +fact);
      
    }
}
